# Development mode (enables /test/* endpoints - set to false in production)
DEV_MODE=false

# API key management (enables /api/keys/* endpoints and X-API-Key auth)
ENABLE_API_KEYS=false

# Fireplace
FIREPLACE_IP=192.168.0.22
FIREPLACE_PORT=2000

# Google OAuth
GOOGLE_CLIENT_ID=<from Google Cloud Console>
GOOGLE_CLIENT_SECRET=<from Google Cloud Console>

# Security
SECRET_KEY=<random 32+ char string for session signing>
ALLOWED_EMAILS=user@gmail.com,other@gmail.com

# App
BASE_URL=https://yourdomain.duckdns.org
